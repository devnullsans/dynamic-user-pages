<h1>
	Tell us about your Business
</h1>
<form name="form1" method="POST" action="/route">
	<table>
		<tr>
			<td>What's the name of your Business?</td>
			<td><input type="text" name="business" required></td>
		</tr>
		<tr>
			<td>What are you Service?</td>
			<td><input type="text" name="service" required></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align: center;"><button>Save</button></td>
		</tr>
	</table>
</form>
<% if(pageID!=null){ %>
	<div> Page Link: <a href="/<%= pageID %>"><%= pageID %></a></div>
<%}%>